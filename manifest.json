{
  "manifest_version": 3,
  "name": "sketch-it",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["p5.js", "p5.dom.js", "content.js"]
    }
  ],
  "permissions": ["tabs", "cookies"],
  "action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "Sketch here."
  }
}
